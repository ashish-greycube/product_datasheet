  "html": "<div style=\"text-align:center;\">\n   <img src=\"{{ frappe.db.get_single_value(\"Website Settings\",\"banner_image\")}}\"></img></div>\n <hr>\n<table>\n<tbody width=\"100%\">\n    \n  <tr>\n     {% if doc.brand %}\n    <td width=\"20%\"><strong>Brand:</strong></td>\n    <td width=\"20%\"><strong>{{doc.brand}} </strong></td>\n    {%else%}\n    <td width=\"20%\">&nbsp;</td>\n    <td width=\"20%\">&nbsp;</td>\n    {% endif %}\n    <td width=\"60%\" colspan=\"2\" rowspan=\"6\">\n     {% if doc.website_image %}\n    <img src=\"{{doc.website_image}}\" height=\"200px\" width=\"300px\"></img>\n      {% endif %}\n    </td>\n  </tr>\n  {% if doc.item_name %}\n  <tr>\n    <td><strong>Item Name:</strong></td>\n    <td><strong><u>{{doc.item_name}}</strong></u></td>\n  </tr>\n  {%else%}\n     <tr></tr>\n   {% endif %}\n   {% if doc.item_sku_number_cf %}\n  <tr>\n    <td><strong>Product SKU Number:</strong></td>\n    <td>{{doc.item_sku_number_cf}}</td>\n  </tr>\n  {%else%}\n     <tr></tr>\n  {% endif %}\n  {% if doc.msrp_cf %}\n  <tr>\n    <td><strong>Product MSRP:</strong></td>\n    <td>{{doc.get_formatted('msrp_cf')}}</td>\n  </tr>\n  {%else%}\n     <tr></tr>\n  {% endif %}\n   {% if doc.country_of_origin %}\n  <tr>\n    <td><strong>Country Of Origin:</strong></td>\n    <td>{{doc.country_of_origin}}</td>\n  </tr>\n  {%else%}\n     <tr></tr>\n  {% endif %}\n  \n   {% if doc.star_ratings_cf   %}\n   <tr>\n     <td><strong>Star Rating:</strong></td>\n      <td>\n    {% for n in range(5) %}\n    {% if n < doc.star_ratings_cf %}\n    <span class=\"fa fa-fw fa-star star-icon\"></span>\n    {% else %}\n    <span class=\"fa fa-fw fa-star-o star-icon\"></span>\n    {% endif %}\n    {% endfor %}\n    </td></tr>\n    {%else%}\n     <tr></tr>\n    {% endif %}\n    \n  <tr>\n    <td colspan=\"2\"><strong>Item Website Description:</strong></td>\n  </tr>\n  \n    {% if doc.slideshow %}\n     <tr>\n    <td colspan=\"2\" rowspan=\"4\"  style=\"text-align:justify;vertical-align:Top;\">{{doc.web_long_description}}</td>\n    <tr>\n    {% for slideshow_item in frappe.get_doc(\"Website Slideshow\",doc.slideshow).slideshow_items %}\n   \t <td rowspan=\"2\">\n\t\t\t\t<img src=\"{{slideshow_item.image}}\" height=\"100px\" width=\"100px\"></img>\n\t</td>\n\t{% if slideshow_item.idx == 2 %}\n\t</tr> <tr>\n  </tr><tr>\n\t{% endif %}\n\t{% endfor %}\n\t\t</tr>\n  {% else %}\n    <tr>\n    <td colspan=\"2\" rowspan=\"4\"style=\"text-align:justify;vertical-align:Top;\">{{doc.web_long_description}}</td>\n    <td rowspan=\"2\"></td>\n    <td rowspan=\"2\"></td>\n  </tr> <tr><td colspan=\"4\"></td></tr>\n  <tr><tr><td colspan=\"4\"></td></tr>\n  <tr>\n  {% endif %}\n  <tr>\n  </tr>\n  <tr><td colspan=\"4\"></td></tr>\n  <tr>\n  <tr>\n    <td  colspan=\"2\" style=\"text-align:justify;\"><strong>Product UPC-A Barcode</strong></td>\n    <td style=\"text-align:justify;\"><strong>Item Page Public URL</strong></td>\n    <td style=\"text-align:justify;\"><strong>Product SDS</strong></td>\n  </tr>\n    <td colspan=\"2\" rowspan=\"2\"> <img src=\"http://www.barcodes4.me/barcode/c128a/{{doc.barcodes[0].barcode}}.png?margin=15&IsTextDrawn=1&IsBorderDrawn=1\" alt=\"barcode UPC-A#\"></img></td>\n    <td rowspan=\"2\"> <img src=\"http://www.barcodes4.me/barcode/qr/{{doc.name}}.png?value={{doc.route}}\" alt=\"{{doc.route}}\"  height=\"150px\" width=\"150px\"></img></td>\n    <td rowspan=\"2\"> <img src=\"http://www.barcodes4.me/barcode/qr/{{doc.item_sku_number_cf}}.png?value={{frappe.get_url()}}{{doc.product_sds_cf}}\" alt=\"{{frappe.get_url()}}{{doc.product_sds_cf}}\"  height=\"150px\" width=\"150px\"></img></td>\n  </tr>\n  <tr>\n  </tr>\n</tbody>\n</table>\n\n<div id=\"footer-html\" class=\"visible-pdf\">\n        {% if not no_letterhead and footer %}\n        <div class=\"letter-head-footer\" style=\"text-align:center;\">\n             <a href=\"{{frappe.get_url()}}\" alt=\"{{frappe.get_url()}}\" >{{frappe.get_url()}}</a>\n        </div>\n        {% endif %}\n    </div>",
